<div class="well">
    <h4>Categories</h4>
    <div class="row">
        <div class="col-lg-12">
            <ul class="list-unstyled">
                {% for category in categories %}
                <li>
                    <a href="{{ path('findByCategory', {'category': category.name}) }}">{{ category.name }}</a>
                </li>
                {% endfor %}
            </ul>
        </div>
    </div>
</div>
